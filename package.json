{
  "name": "@nayumin/magmastream",
  "version": "2.3.0",
  "description": "A user-friendly Lavalink client designed for NodeJS, added keepAlive for heroku",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "types": "rtb --dist dist",
    "lint": "eslint --ext .ts ./src",
    "ci": "run-s lint build types"
  },
  "devDependencies": {
    "@favware/rollup-type-bundler": "^3.2.1",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "npm-run-all": "^1.8.0",
    "typedoc": "^0.25.7",
    "typedoc-plugin-no-inherit": "^1.4.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@discordjs/collection": "^2.0.0",
    "axios": "^1.6.5",
    "events": "^3.3.0",
    "lodash": "^4.17.21",
    "tslib": "^2.6.2",
    "ws": "^8.16.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "object-curly-spacing": [
        "error",
        "always"
      ]
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ]
  },
  "keywords": [
    "lavalink client",
    "wrapper",
    "typescript",
    "discord.js",
    "node.js",
    "java",
    "javascript",
    "audio streaming",
    "music bot",
    "voice chat",
    "discord integration",
    "high performance",
    "scalable",
    "easy-to-use",
    "feature-rich",
    "cross-platform",
    "seamless integration",
    "community support",
    "documentation",
    "open-source",
    "lavalink",
    "magmastream"
  ],
  "repository": {
    "url": "https://github.com/nayumin/magmastream/tree/main"
  },
  "homepage": "https://nayu.xyz",
  "author": "Nayumi",
  "license": "Apache-2.0"
}
